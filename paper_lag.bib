@article{fisherDynamicModelPsychological2015,
  title = {Toward a Dynamic Model of Psychological Assessment: {{Implications}} for Personalized Care},
  shorttitle = {Toward a Dynamic Model of Psychological Assessment},
  author = {Fisher, Aaron J.},
  year = {2015},
  month = aug,
  journal = {Journal of Consulting and Clinical Psychology},
  volume = {83},
